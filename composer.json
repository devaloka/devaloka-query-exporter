{
    "name"             : "devaloka/devaloka-query-exporter",
    "description"      : "Exports WP_Query's query variables to front-end as JSON",
    "version"          : "0.1.0",
    "type"             : "wordpress-plugin",
    "keywords"         : [
        "wordpress",
        "plugin",
        "wp_query"
    ],
    "homepage"         : "https://github.com/devaloka/devaloka-query-exporter",
    "license"          : [
